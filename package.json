    "@nestjs/core": "^7.0.0",
    "@nestjs/platform-express": "^7.5.1",
    "@nestjs/serve-static": "^2.1.3",
    "@nestjs/swagger": "^4.6.1",
    "@ng-bootstrap/ng-bootstrap": "8.0.0",
    "chart.js": "2.8.0",
    "concurrently": "^5.3.0",
    "core-js": "3.6.4",
    "express": "^4.17.1",
    "firebase-admin": "^9.3.0",
    "mongodb": "^3.6.2",
    "ngx-toastr": "13.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "router": "1.3.5",
